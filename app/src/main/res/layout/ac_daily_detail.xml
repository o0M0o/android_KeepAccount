<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="wxm.KeepAccount.ui.acutility.ACDailyDetail"
    android:focusable="true">

    <ListView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lv_daily_detail"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:focusable="true"
        android:listSelector="@drawable/daily_detail_listitem_bg"
        android:cacheColorHint="@android:color/transparent"
        android:divider="@color/black"
        android:dividerHeight="1dp"/>


    <cn.wxm.andriodutillib.capricorn.RayMenu
        android:id="@+id/rm_record_dailydetail"
        app:childSize="36dp"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="5dp"
        android:paddingRight="120dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="30dp" />
</RelativeLayout>
