<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wxm.KeepAccount">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <uses-sdk android:minSdkVersion="11" />
    <application
        android:name=".BaseLib.ContextUtil"
        android:allowBackup="true"
        android:icon="@mipmap/ic_share"
        android:label="@string/cn_applabel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".ui.activities.ActivityDailyDetail" />
        <activity android:name=".ui.activities.ActivityAddRecord" />
        <activity
            android:name=".ui.activities.ActivityStart"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="@string/title_activity_start">
        </activity>
        <activity
            android:name=".ui.activities.ActivityLogin"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activities.ActivityAddUsr"
            android:label="@string/title_activity_add_account">
        </activity>
        <activity
            android:name=".ui.activities.ActivityMainTest"
            android:label="@string/title_activity_activity_main_test"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>